{
	"composerBinDir": "vendor\/bin",
	"projectName": "Stepup Saml Bundle",
	"buildArtifacts": {
		"enabled": false
	},
	"travis": {
		"enabled": true,
		"phpVersions": {
			"1": "5.6",
			"2": "5.5",
			"3": "5.4"
		},
		"env": {
			"enabled": true,
			"vars": [
				"SYMFONY_ENV=test"
			]
		},
		"slack": {
			"enabled": false
		}
	},
	"enablePhpTools": true,
	"enableComposer": false,
	"enablePhpLint": true,
	"enablePhpMessDetector": true,
	"phpMdExcludePatterns": [

	],
	"enablePhpCodeSniffer": true,
	"phpCodeSnifferCodingStyle": "PSR2",
	"phpCsExcludeSymfony": false,
	"phpCsExcludeCustomPatterns": [

	],
	"phpCsExcludePatterns": [

	],
	"enablePhpCopyPasteDetection": true,
	"phpCpdExcludePatterns": [

	],
	"enablePhpSecurityChecker": true,
	"phpSrcPath": [
		"src"
	],
	"enablePhpUnit": true,
	"customPhpUnitXml": true,
	"phpUnitConfigPath": "phpunit.xml",
	"enableJsTools": false,
	"enableJsHint": false,
	"enableBehat": false,
	"enablePreCommitHook": true,
	"md5Command": "md5sum",
	"_qa_tools_run_completed": true
}